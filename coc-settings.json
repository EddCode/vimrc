{
  "suggest.echodocSupport": true,
  "suggest.maxCompleteItemCount": 20,
  "coc.preferences.formatOnSaveFiletypes": [
    "javascript",
    "go",
    "json",
    "javascriptreact",
    "dart"
  ],
  "eslint.filetypes": ["javascript", "go", "javascriptreact", "dart"],
  "diagnostic.errorSign": "ˣ",
  "diagnostic.warningSign": "⚠",
  "diagnostic.infoSign": "ℹ",
  "languageserver": {
    "dockerfile": {
      "command": "docker-langserver",
      "filetypes": ["dockerfile"],
      "args": ["--stdio"]
    }
  },
  "flutter.UIPath": true,
  "flutter.enabled": true,
  "flutter.provider.enableSnippet": true,
  "flutter.lsp.initialization.closingLabels": true,
  "flutter.outlineWidth": 30,
  "flutter.outlineIconPadding": 0
}
